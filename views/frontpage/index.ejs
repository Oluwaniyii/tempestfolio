      <!-- Blogs End -->
      
      <!-- Contact me Start -->
      <section id="contact-me">
        <div class="container">
          <div class="section-heading">
            <div class="title text-lg text-dark">Contact Me</div>
            <div class="comment text-sm text-secondary">
             Contact me for fulltime or partime jobs, freelancing or project collabration. I reply in a flash 
            </div>
          </div>

          <div class="contact">
            <div class="phone">
              <span class="icon font-600">
                <i class="fas fa-phone-alt"></i>
              </span>
              <span class="font-600 text-md text-dark mb-1">Phone</span>
              <span class="text-secondary">
               If its urgent, be sure to call or text me on whatsapp, I'm one text away :)
              </span>
              <span class="main copy-to-clipboard" role="button" onclick="clipText('+2349074237582')">+234-907-423-7582 </span>
            </div>
            <div class="email">
              <span class="icon">
                <i class="far fa-envelope"></i>
              </span>
              <span class="font-600 text-md text-dark mb-1">Email</span>
              <span class="text-secondary">
                I will check emails at my convenient time. Please use the mail if it's not urgent
              </span>
              <span class="main copy-to-clipboard" onclick="clipText('oluwaniyiiofficial@gmail.com')">oluwaniyiiofficial@gmail.com</span>
            </div>
          </div>

          <div class="direct-mail">
            <form action="">
              <div class="row">
                <div class="left">
                  <input type="text" id="mail-name" placeholder="Your Firstname :" />
                  <input type="text" id="mail-address" placeholder="Your Email :" />
                  <input type="text" id="mail-subject" placeholder="Your Subject :" />
                </div>
                <div class="right">
                  <textarea
                    name=""
                    id="mail-body"
                    cols="30"
                    rows="10"
                    placeholder="Your Message :"
                  ></textarea>
                </div>
              </div>
              <div class="submit">
                <button type="button" onclick="sendMail()" class="btn btn-primary">Send</button>
              </div>
            </form>
          </div>
        </div>
      </section>
      <!-- Contact me End -->
      <!-- Social Start -->
